                        <ul class="block-wrap-list">

                            <li class="block-wrap">
                                <div class="unit unit-sm-horizontal unit-align-center unit-middle unit-spacing-sm">
                                    <div class="unit-left">
                                        <figure><img src="../../static/img/icon-phone-28x29.png" alt="" width="28" height="29"></figure>
                                    </div>
                                    <div class="unit-body">
                                        <address class="contact-info">
                                            <span class="text-bold">
                                                <a href="callto:#"
                                                   class="text-gray-base">8-800-707-10-00</a>
                                            </span><span>
                                                <a href="mailto:info@invoice.cafe">info@invoice.cafe</a>
                                            </span>
                                        </address>
                                    </div>
                                </div>


                            </li>
                            <li class="block-wrap">

                                <div class="unit unit-sm-horizontal unit-align-center unit-middle unit-spacing-sm">

                                    <div class="unit-body">
                                        <a href="" class="btn btn-sm btn-curious-blue-outline">Заказать звонок</a>
                                    </div>
                                </div>
                            </li>

                        </ul>

                    </div>
                </div>
                <div class="rd-navbar-nav-wrap rd-navbar-nav-dark">
                    <div class="rd-navbar-nav-wrap-inner">
                        <!-- RD Navbar Search-->
                        <div class="pull-right">




                            <ul class="rd-navbar-nav rd-navbar-nav-1">

                                <li class="rd-navbar--has-dropdown rd-navbar-submenu">
                                    <a href="#">@ViewBag.CompanyName</a><!-- RD Navbar Dropdown-->

                                    <span class="rd-navbar-submenu-toggle"></span>
                                </li>
                                <li>
                                    <a href="#">
                                        <span class="hidden-lg hidden-md">
                                            Выход
                                        </span>
                                        <div class="rd-navbar-exit hidden-sm hidden-xs">

                                        </div>

                                    </a><!-- RD Navbar Dropdown-->

                                </li>


                            </ul>

                        </div>

                        <!-- RD Navbar Nav-->


                    </div>
                </div>
            </div>
        </nav>
